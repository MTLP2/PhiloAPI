{
  "name": "diggersfactory-api",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "node ace serve --watch",
    "build": "node ace build --production --ignore-ts-errors",
    "install:prod": "npm ci --production",
    "start": "node server.js",
    "lint": "eslint . --ext=.ts",
    "format": "prettier --write .",
    "test": "jest",
    "test:watch": "jest --watch"
  },
  "eslintConfig": {
    "extends": [
      "plugin:adonis/typescriptApp",
      "prettier"
    ],
    "plugins": [
      "prettier"
    ],
    "rules": {
      "comma-dangle": "off",
      "@typescript-eslint/explicit-member-accessibility": "off",
      "handle-callback-err": "off",
      "prettier/prettier": [
        "error"
      ]
    }
  },
  "eslintIgnore": [
    "build"
  ],
  "prettier": {
    "trailingComma": "none",
    "semi": false,
    "singleQuote": true,
    "useTabs": false,
    "quoteProps": "consistent",
    "bracketSpacing": true,
    "arrowParens": "always",
    "printWidth": 100
  },
  "devDependencies": {
    "@adonisjs/assembler": "^5.8.1",
    "@japa/preset-adonis": "^1.2.0",
    "@japa/runner": "^2.1.3",
    "@types/jest": "^29.5.14",
    "adonis-preset-ts": "^2.1.0",
    "eslint": "^8.23.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-adonis": "^2.1.0",
    "eslint-plugin-prettier": "^4.2.1",
    "jest": "^29.7.0",
    "pino-pretty": "^9.1.0",
    "prettier": "^2.7.1",
    "ts-jest": "^29.2.6",
    "typescript": "~4.6",
    "youch": "^3.2.2",
    "youch-terminal": "^2.1.5"
  },
  "dependencies": {
    "@adonisjs/core": "^5.8.6",
    "@adonisjs/i18n": "^1.5.6",
    "@adonisjs/view": "^6.1.6",
    "aws-sdk": "^2.1198.0",
    "bcryptjs": "^2.4.3",
    "bwip-js": "^3.1.0",
    "canvas": "^2.10.2",
    "customerio-node": "^3.3.6",
    "exceljs": "^4.3.0",
    "fast-xml-parser": "^4.3.2",
    "fluent-ffmpeg": "^2.1.2",
    "google-auth-library": "^8.6.0",
    "hashids": "^2.2.10",
    "html-pdf": "^3.0.1",
    "intercom-client": "^3.1.5",
    "jsdom": "^24.0.0",
    "jsonwebtoken": "^8.5.1",
    "jszip": "^3.10.1",
    "juice": "^8.1.0",
    "knex": "^2.2.0",
    "kysely": "^0.27.4",
    "marked": "^4.3.0",
    "md5": "^2.3.0",
    "moment": "^2.29.4",
    "mysql": "^2.18.1",
    "mysql2": "^3.9.7",
    "node-vibrant": "^3.2.1-alpha.1",
    "paypal-rest-sdk": "^1.8.1",
    "postcode-validator": "^3.7.0",
    "proxy-addr": "^2.0.7",
    "reflect-metadata": "^0.1.13",
    "request": "^2.88.2",
    "sharp": "^0.30.7",
    "sitemap": "^7.1.1",
    "soap": "^1.0.0",
    "source-map-support": "^0.5.21",
    "ssh2-sftp-client": "^9.0.4",
    "stripe": "^10.2.0",
    "ua-parser-js": "^1.0.2",
    "underscore": "^1.13.4"
  }
}
